{
  "name": "products-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev:api": "tsnd --poll --transpile-only --respawn --ignore-watch node_modules src/api.ts",
    "dev:worker": "tsnd --poll --transpile-only --respawn --ignore-watch node_modules src/worker.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test:coverage": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "start:api": "node dist/api.js",
    "start:worker": "node dist/worker.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -d src/infra/postgres/DataSource.ts",
    "migrate:run": "npm run typeorm migration:run",
    "build": "tsc"
  },
  "keywords": [],
  "author": "Wilson Carugatti",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1160.0",
    "bull": "^4.8.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "multer": "^1.4.5-lts.1",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "pg-mem": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  }
}
